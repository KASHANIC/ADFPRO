{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-sina"
		},
		"LS_to_DB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_to_DB'"
		},
		"LS_to_datalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_to_datalake'"
		},
		"LS_to_DB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "server-sk-test.database.windows.net"
		},
		"LS_to_DB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "database-test"
		},
		"LS_to_DB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "kashani"
		},
		"LS_to_datalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storagedatafactorysina.dfs.core.windows.net/"
		},
		"RestService1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://pokeapi.co/api/v2/pokemon"
		},
		"trigger1_properties_Schedule_parameters_schema": {
			"type": "string",
			"defaultValue": "source"
		},
		"trigger1_properties_Schedule_parameters_table": {
			"type": "string",
			"defaultValue": "Orders"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-sina_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_to_DB_password": {
						"value": "[parameters('LS_to_DB_password')]"
					},
					"LS_to_datalake_accountKey": {
						"value": "[parameters('LS_to_datalake_accountKey')]"
					},
					"LS_to_DB_properties_typeProperties_server": {
						"value": "[parameters('LS_to_DB_properties_typeProperties_server')]"
					},
					"LS_to_DB_properties_typeProperties_database": {
						"value": "[parameters('LS_to_DB_properties_typeProperties_database')]"
					},
					"LS_to_DB_properties_typeProperties_userName": {
						"value": "[parameters('LS_to_DB_properties_typeProperties_userName')]"
					},
					"LS_to_datalake_properties_typeProperties_url": {
						"value": "[parameters('LS_to_datalake_properties_typeProperties_url')]"
					},
					"RestService1_properties_typeProperties_url": {
						"value": "[parameters('RestService1_properties_typeProperties_url')]"
					},
					"trigger1_properties_Schedule_parameters_schema": {
						"value": "[parameters('trigger1_properties_Schedule_parameters_schema')]"
					},
					"trigger1_properties_Schedule_parameters_table": {
						"value": "[parameters('trigger1_properties_Schedule_parameters_table')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}